package in.debasish.VariablesandMethods;

public class MethodCallusingThis {

	public MethodCallusingThis(int i, int j, int k) {

	}

	public void m1() {

		System.out.println("Default Method");
	}

	public void m2(int a) {
		System.out.println("One Parameterized Method");
	}

	public void m3(int a, int b) {
		System.out.println("Two Parameterized Method");
	}

	public void m4(int a, int b, int c) {
		this.m1();
		System.out.println("Three Parameterized Method");
	}

	public void m5(int a, int b, int c, int d) {

		System.out.println("Four Parameterized Method");
	}

	public static void main(String[] args) {

		MethodCallusingThis mth=new MethodCallusingThis(12,32,54);
		
	}

}
